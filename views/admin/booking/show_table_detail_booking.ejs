
                        <div class="card shadow mb-4 mt-2">
                            <div class="card-header py-3">
                                <a href="/admin/booking" class="btn btn-warning">Back</a>
                               
                              </div>
                            <div class="card-body">
                              <div class="row">
                                  <div class="col-md-4">
                                    <img src="http://localhost:3000/<%= booking.payments.proofPayment %>" alt="image"
                                    style="width: 100%; display: block;">
                                    <div class="row ml-1">
                                    <form action="/admin/bank/?_method=DELETE" method="POST" >
                                        <button type="submit" class="btn btn-success btn-sm">Confirmation <i class="fas fa-check"></i></button>
                                      </form>
                                      <form action="/admin/bank/?_method=DELETE" method="POST" class="ml-2">
                                        <button type="submit" class="btn btn-danger btn-sm">Reject <i class="fas fa-times"></i></button>
                                      </form>
                                    </div>
                                  </div>
                                  <div class="col-md-8">
                                    <div class="table-responsive">
                                        <table class="table table-bordered" id="dataTableActivity" width="100%" cellspacing="0">
                                      <thead>
                                        <tr>
                                          <th>member Name</th>
                                          <th>Total</th>
                                          <th>Bank From</th>
                                          <th>Title</th>
                                        </tr>
                                      </thead>
                                      <tbody>
                                          <td><%= booking.memberId.firstName %> <%= booking.memberId.LastName %></td>
                                          <td>$ <%= booking.total %></td>
                                          <td><%= booking.payments.bankFrom %></td>
                                          <td><%= booking.itemId.title %></td>
                                        </tbody>
                                    </table>
                                  </div>
                                  </div>
                              </div>

                          </div>
                        </div>